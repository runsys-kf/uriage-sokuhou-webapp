<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="ReverseProxyInboundRule" stopProcessing="true">
          <match url="(.*)" />
          <action type="Rewrite" url="https://salesrepo.runsystem.co.jp/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
